{
    "name": "quanta/container",
    "type": "library",
    "description": "Minimalist dependency injection container implementing Psr-11",
    "keywords": [
        "psr-11",
        "container",
        "dependency-injection"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quanta/container",
    "support": {
        "issues": "https://github.com/quanta/container/issues",
        "source": "https://github.com/quanta/container"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "psr/container": "^1.0",
        "quanta/exceptions": "^0.4.0"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^2.0",
        "kahlan/kahlan": "^4.0",
        "phpstan/phpstan": "^0.10.8"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\": "src"
        }
    }
}
