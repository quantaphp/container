{
    "name": "quanta/container",
    "type": "library",
    "description": "Minimalist dependency injection container implementing Psr-11",
    "keywords": [
        "psr-11",
        "container",
        "dependency-injection"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quantaphp/container",
    "support": {
        "issues": "https://github.com/quantaphp/container/issues",
        "source": "https://github.com/quantaphp/container"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^4.0",
        "kahlan/kahlan": "^5.0",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\": "src"
        }
    }
}
